<?php

/**
 * @file
 * Functions to support theming in the Bookclub theme.
 */

/**
 * Implements hook_preprocess_HOOK() for the blog content type.
 *
 * Attaches the blog css/js libraries from from bookclub theme.
 */
function bookclub_preprocess_node__blog(&$variables) {
  $variables['#attached']['library'][] = 'bookclub/blog';
}
